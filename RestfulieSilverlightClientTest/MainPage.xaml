    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="800">

    <Grid x:Name="LayoutRoot" Background="White">
        <TextBox Height="236" HorizontalAlignment="Left" Margin="12,12,0,0" Name="Message" VerticalAlignment="Top" Width="776" VerticalScrollBarVisibility="Auto" />
        <Button Content="Button" Height="23" HorizontalAlignment="Left" Margin="12,265,0,0" Name="Go" VerticalAlignment="Top" Width="75" Click="Go_Click" />
    </Grid>
</UserControl>
